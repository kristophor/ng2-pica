{
    "name": "ng6-pica",
    "version": "0.0.2",
    "description": "Angular 6 fixed for wrapper for pica to resize images.",
    "repository": {
        "type": "git",
        "url": "https://github.com/kristophor/ng2-pica.git"
    },
    "license": "MIT",
    "module": "./dist/ng6-pica.js",
    "main": "./dist/bundles/ng6-pica.umd.js",
    "typings": "./dist/ng6-pica.d.ts",
    "scripts": {
        "build": "npm run transpile && npm run package && npm run minify",
        "minify": "uglifyjs dist/bundles/ng6-pica.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng6-pica.umd.min.js",
        "package": "rollup -c",
        "transpile": "ngc"
    },
    "author": {
        "name": "Kris Chen",
        "email": "email@krischen.ca"
    },
    "keywords": [
        "angular",
        "angular2",
        "pica",
        "image",
        "resize"
    ],
    "dependencies": {
        "exif-js": "^2.3.0",
        "pica": ">=4.0.1"
    },
    "peerDependencies": {
        "@angular/core": ">=6.0.0",
        "@angular/common": ">=6.0.0",
        "@angular/platform-browser": ">=6.0.0",
        "rxjs": ">=6.0.0"
    },
    "devDependencies": {
        "@angular/common": "^6.0.3",
        "@angular/compiler": "^6.0.3",
        "@angular/compiler-cli": "^6.0.3",
        "@angular/core": "^6.0.3",
        "@angular/platform-browser": "^6.0.3",
        "@angular/platform-browser-dynamic": "^6.0.3",
        "@angular/router": "^6.0.3",
        "@types/node": "^10.3.0",
        "core-js": "^2.5.7",
        "reflect-metadata": "^0.1.12",
        "rollup": "^0.41.4",
        "rxjs": "^6.2.0",
        "tslint": "^4.4.2",
        "typescript": ">=2.7.2 <2.8 ",
        "uglify-js": "^2.7.5",
        "zone.js": "~0.8.26"
    },
    "bugs": {
        "url": "https://github.com/kristophor/ng2-pica/pulls"
    },
    "homepage": "https://github.com/kristophor/ng2-pica/blob/master/README.md"
}
